{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "include": ["src"],
  "compilerOptions": {
    "noEmit": true,
    "verbatimModuleSyntax": true,
    "noUncheckedIndexedAccess": false,
    "exactOptionalPropertyTypes": false,
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "types": ["@types/node", "@types/vscode"],
  },
}
